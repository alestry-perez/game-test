#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/alestry-perez/Desktop/vsc-projects/game-test/node_modules/.pnpm/mime@1.6.0/node_modules/mime/node_modules:/Users/alestry-perez/Desktop/vsc-projects/game-test/node_modules/.pnpm/mime@1.6.0/node_modules:/Users/alestry-perez/Desktop/vsc-projects/game-test/node_modules/.pnpm/node_modules:/Users/alestry-perez/Desktop/vsc-projects/game-test/node_modules:/Users/alestry-perez/Desktop/vsc-projects/node_modules:/Users/alestry-perez/Desktop/node_modules:/Users/alestry-perez/node_modules:/Users/node_modules:/node_modules:/Users/alestry-perez/Desktop/vsc-projects/game-test/node_modules/.pnpm/node_modules/mime/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/cli.js" "$@"
else
  exec node  "$basedir/../mime/cli.js" "$@"
fi
